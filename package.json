{
  "name": "vclub",
  "repository": "https://github.com/VirtualClub/vclub",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/server.bundle.js",
    "storybook": "start-storybook -p 9001 -s ./dist-files/public",
    "clean": "rimraf dist",
    "build": "npm run build:prod",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --colors --progress --optimize-minimize --optimize-occurence-order --optimize-dedupe --devtool sourcemap",
    "build:dev": "webpack --colors --progress --devtool eval --display-error-details",
    "build:watch": "npm run build:dev -- --watch",
    "lint": "eslint ./src",
    "autofix": "eslint ./src --fix",
    "test": "jest",
    "check": "npm run lint && npm run test",
    "watch:test": "jest --watch --no-cache",
    "coverage:test": "jest --coverage"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "borex-actions": "^0.1.0",
    "borex-reducers": "^0.1.0",
    "classnames": "^2.2.5",
    "date-fns": "^1.15.1",
    "eslint": "^3.8.1",
    "express": "^4.14.0",
    "konva": "^1.2.2",
    "ramda": "^0.22.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-konva": "^1.1.0",
    "react-motion": "^0.4.5",
    "react-motion-ui-pack": "^0.9.0",
    "react-redux": "^4.4.5",
    "recompose": "^0.20.2",
    "redux": "^3.6.0",
    "redux-form": "^6.0.5",
    "reselect": "^2.5.4",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "uuid": "^2.0.3",
    "webrtc-adapter": "^2.0.8"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-borex-autotype": "^0.1.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "enzyme-to-json": "^1.3.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^16.0.1",
    "postcss-autoreset": "^1.1.5",
    "postcss-loader": "^0.13.0",
    "postcss-normalize": "^0.3.0",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "rimraf": "^2.5.4",
    "stack-source-map": "^1.0.5",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "0.0.31",
    "updeep": "^0.16.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-hot-middleware": "^2.12.2"
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^vclub(.*)$": "<rootDir>/src$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
